{
  "name": "ghas-licence-clean",
  "version": "1.0.0",
  "description": "A GitHub Action that helps clean up GHAS Licences",
  "main": "main.ts",
  "scripts": {
    "build": "tsc && ncc build ./src/main.ts -o dist",
    "prettier:write": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint --fix . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install",
    "local": "yarn run build && act --job dispatch --env-file ./.env --eventpath .github/workflows/regression/payload.event --secret-file my.secrets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/artifact": "^1.1.2",
    "@actions/core": "^1.10.1",
    "@actions/github": "^5.1.1",
    "@octokit/action": "^6.0.6",
    "@octokit/plugin-retry": "^6.0.1",
    "@octokit/plugin-throttling": "^8.0.0",
    "@vercel/ncc": "^0.38.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.6",
    "@types/json-schema": "^7.0.13",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "18"
  }
}
