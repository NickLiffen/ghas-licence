{
  "name": "ghas-licence-clean",
  "version": "1.0.0",
  "description": "A GitHub Action that helps clean up GHAS Licences",
  "main": "main.ts",
  "scripts": {
    "build": "tsc && ncc build ./src/main.ts -o dist",
    "prettier:write": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint --fix . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install",
    "local": "yarn run build && act --job dispatch --env-file ./.env --eventpath .github/workflows/regression/payload.event --secret-file my.secrets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/artifact": "^2.1.7",
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/action": "^7.0.0",
    "@octokit/plugin-retry": "^7.1.1",
    "@octokit/plugin-throttling": "^9.3.0",
    "@vercel/ncc": "^0.38.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": "20"
  }
}
