{
  "name": "ghas-licence-clean",
  "version": "1.0.0",
  "description": "A GitHub Action that helps clean up GHAS Licences",
  "main": "main.ts",
  "scripts": {
    "build": "tsc && ncc build ./src/main.ts -o dist",
    "prettier:write": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint --fix . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install",
    "local": "yarn run build && act --job dispatch --env-file ./.env --eventpath .github/workflows/regression/payload.event --secret-file my.secrets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/artifact": "^1.1.1",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/action": "^5.0.2",
    "@octokit/plugin-retry": "^4.1.3",
    "@octokit/plugin-throttling": "^5.1.1",
    "@vercel/ncc": "^0.36.1",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "18"
  }
}
