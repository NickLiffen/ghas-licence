{
  "name": "ghas-licence-clean",
  "version": "1.0.0",
  "description": "A GitHub Action that helps clean up GHAS Licences",
  "main": "main.ts",
  "scripts": {
    "build": "tsc && ncc build ./src/main.ts -o dist",
    "prettier:write": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint:check": "eslint . --ext .ts",
    "lint:write": "eslint --fix . --ext .ts",
    "test": "jest --coverage",
    "prepare": "husky install",
    "local": "yarn run build && act --job regression --env-file ./.env --eventpath .github/workflows/regression/payload.event --secret-file my.secrets"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@actions/artifact": "^1.1.0",
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.3",
    "@octokit/action": "^3.18.1",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.7.0",
    "@vercel/ncc": "^0.34.0",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.3",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "16"
  }
}
